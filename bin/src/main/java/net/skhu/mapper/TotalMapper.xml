<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.skhu.mapper.TotalMapper">
	<select id="find" resultType="Total">
		SELECT * FROM total
	</select>
	<update id="update">
		UPDATE total SET grade = #{grade}
	</update>
</mapper>