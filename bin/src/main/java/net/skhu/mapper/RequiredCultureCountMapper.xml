<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.skhu.mapper.RequiredCultureCountMapper">
	<select id="find" resultType="RequiredCultureCount">
		SELECT * FROM requiredculture_count
	</select>
	<update id="update"> 
		UPDATE requiredculture_count SET chapelCount=#{chapelCount}, serveCount=#{serveCount}
	</update>
</mapper>