<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.skhu.mapper.CultureMapper">
	<select id="find" resultType="Culture">
		SELECT * FROM culture
	</select>
	<select id="findCulture" resultType="Culture">
		SELECT * FROM culture
	</select>
	<update id="update">
		UPDATE culture SET grade = #{grade}
	</update>
</mapper>