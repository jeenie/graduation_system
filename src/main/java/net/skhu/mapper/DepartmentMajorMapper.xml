<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.skhu.mapper.DepartmentMajorMapper">
	<select id="findAll" resultType="DepartmentMajor">
		Select * From Department_Major
	</select>
	<select id="findMajorsByDepartmentId" resultType="DepartmentMajor">
		Select * From Department_Major
		Where departmentId=#{departmentId}
	</select>
</mapper>